<input type="hidden" name="room-id" id="room-id" value="<%= roomId %>" />

<div class="grid">
    <div><h2>Available Games</h2></div>
    <div>
      <h2>Chat</h2>
      <article id="chat-window">
        
      </article>
      <input
        type="text"
